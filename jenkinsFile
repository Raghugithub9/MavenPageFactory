pipeline{
	agent any

	tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "M3"
    }

	
	stages{
	stage('Preparation'){
	steps{
	git 'https://github.com/Raghugithub9/TestNG-Maven-SampleProject.git'
	}
	}
	stage('Build'){
	steps{
	bat "mvn clean test"
	}
           }
        }
}
